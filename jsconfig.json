
{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "checkJs": true,
      "jsx": "preserve",
      "allowSyntheticDefaultImports": true,
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "exclude": [
      "node_modules",
      "**/node_modules/*"
    ],
    "include": [
      "./**/*"
    ]
  }
  